<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_height="wrap_content">

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:orientation="horizontal"
            android:paddingVertical="15dp">
        <RelativeLayout
                android:id="@+id/rl_nav_category_main_portfolio_tab1"
                android:layout_width="55dp"
                android:layout_height="match_parent"
                android:gravity="center">
            <TextView
                    android:text="전체"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:textSize="14dp" android:textColor="#484848"
                    android:textStyle="bold"/>
        </RelativeLayout>

        <RelativeLayout
                android:id="@+id/rl_nav_category_main_portfolio_tab2"
                android:layout_width="55dp"
                android:layout_height="match_parent"
                android:gravity="center">
            <TextView
                    android:text="동아리"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:textSize="14dp" android:textColor="#484848"
                    android:textStyle="bold"/>
        </RelativeLayout>

        <RelativeLayout
                android:id="@+id/rl_nav_category_main_portfolio_tab3"
                android:layout_width="55dp"
                android:layout_height="match_parent"
                android:gravity="center">
            <TextView
                    android:text="인턴"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:textSize="14dp" android:textColor="#484848"
                    android:textStyle="bold"/>
        </RelativeLayout>

        <RelativeLayout
                android:id="@+id/rl_nav_category_main_portfolio_tab4"
                android:layout_width="55dp"
                android:layout_height="match_parent"
                android:gravity="center">
            <TextView
                    android:text="대외활동"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:textSize="14dp" android:textColor="#484848"
                    android:textStyle="bold"/>
        </RelativeLayout>

        <RelativeLayout
                android:id="@+id/rl_nav_category_main_portfolio_tab5"
                android:layout_width="55dp"
                android:layout_height="match_parent"
                android:gravity="center">
            <TextView
                    android:text="공모전"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:textSize="14dp" android:textColor="#484848"
                    android:textStyle="bold"/>
        </RelativeLayout>

        <RelativeLayout
                android:id="@+id/rl_nav_category_main_portfolio_tab6"
                android:layout_width="55dp"
                android:layout_height="match_parent"
                android:gravity="center">
            <TextView
                    android:text="봉사활동"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:textSize="14dp" android:textColor="#484848"
                    android:textStyle="bold"/>
        </RelativeLayout>

    </LinearLayout>

</LinearLayout>